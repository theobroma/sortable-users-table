{
  "name": "React-es6-starter",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run clean && npm run dll && npm run dev",
    "dll": "webpack --config webpack.dll.js --progress --colors --inline",
    "dev": "webpack-dev-server --mode development --history-api-fallback --inline --progress --colors --port 3000 --config webpack.dev.js",
    "build:stats": "webpack --env production --profile --progress --json > stats.json",
    "build": "npm run clean && npm run build:prod",
    "build:prod": "webpack --mode production --config webpack.prod.js",
    "clean": "rimraf ./dist"
  },
  "dependencies": {
