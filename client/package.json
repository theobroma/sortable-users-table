    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "sass-loader": "^7.1.0",
    "semantic-ui-react": "^0.86.0",
    "shortid": "^2.2.14",
    "style-loader": "^0.23.1",
    "styled-components": "^4.2.0",
